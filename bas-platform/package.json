{
	"name": "bas-platform",
	"displayName": "BAS Platform Extension",
	"description": "SAP Business Application Studio platform extension",
	"version": "0.0.1",
	"publisher": "SAPOSS",
	"engines": {
		"vscode": "^1.44.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/guided-development",
		"directory": "bas-platform"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"types": "./out/api",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"title": "Actions",
			"properties": {
				"actions": {
					"type": "array"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package ."
	},
	"devDependencies": {
		"@types/vscode": "^1.40.0",
		"typescript": "^3.9.7",
		"vsce": "^1.69.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {}
}
